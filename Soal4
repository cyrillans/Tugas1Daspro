program saya;
uses crt;
var
    Jalan,No,Kota,KodePos:string;
begin
    clrscr;
    write('Jalan :'); readln(Jalan);
    write('No :'); readln(No);
    write('Kota :'); readln(Kota);
    write('Kode Pos :'); readln(KodePos);
    write('Alamat : Jalan ',Jalan);
    write(' No.',No);
    write(' ',Kota);
    write(' ',KodePos);
end.
